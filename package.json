{
  "name": "layouts-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run index-watch\" \"next dev -p 3002\"",
    "build": "next build",
    "prebuild": "npm run index-content",
    "start": "npm run index-content && next start",
    "lint": "next lint",
    "index-watch": "nodemon --watch 'src/docs/essentials' --ext 'js,json,md,mdx' --exec 'npm run index-content'",
    "index-content": "node ./scripts/mdxToJson.js && node ./scripts/htmlIntoJson.js && node ./scripts/layoutsIntoJson.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@mintlify/components": "^0.3.15",
    "@mintlify/mdx": "^0.0.46",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@types/mdx": "^2.0.13",
    "check-types": "^11.2.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.429.0",
    "next": "14.2.5",
    "next-mdx-remote": "^5.0.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^18",
    "react-code-block": "^1.0.0",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-use": "^17.5.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "@next/mdx": "^14.2.5",
    "@types/check-types": "^11.2.2",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/nlcst": "^2.0.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/unist": "^3.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "nodemon": "^3.1.4",
    "postcss": "^8",
